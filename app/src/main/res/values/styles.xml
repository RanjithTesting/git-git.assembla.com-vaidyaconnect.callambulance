        <item name="colorControlHighlight">@color/separator</item>
        <item name="android:dividerHeight">0.75dp</item>
    </style>

    <style name="ActivityDialogAllergyTypesTheme" parent="Theme.AppCompat.Light.Dialog">
        <item name="colorControlNormal">@color/colorPrimary</item>
        <item name="colorControlActivated">@color/colorPrimary</item>
        <item name="colorControlHighlight">@color/colorPrimary</item>
        <item name="android:windowBackground">@color/white</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:excludeFromRecents">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowActionModeOverlay">true</item>
    </style>
